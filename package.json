{
  "private": true,
  "name": "slick-router",
  "version": "1.0.0",
  "description": "A flexible hierarchical client side router",
  "main": "slick-router.js",
  "module": "slick-router.js",
  "scripts": {
    "build": "standard | snazzy && node tasks/build.js",
    "lint": "standard",
    "test-coverage": "karma start --browsers ChromeHeadless --single-run",
    "test": "karma start --browsers ChromeHeadless --single-run --no-coverage",
    "test:debug": "karma start --browsers ChromeDebugging --no-single-run --no-coverage"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/blikblum/slick-router.git"
  },
  "author": "Luiz Américo Pereira Câmara",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/blikblum/slick-router/issues"
  },
  "dependencies": {
    "path-to-regexp-es": "^3.0.0"
  },
  "keywords": [
    "router",
    "history",
    "browser",
    "pushState",
    "hierarchical",
    "nested"
  ],
  "devDependencies": {
    "@sinonjs/referee": "^3.2.0",
    "@sinonjs/referee-sinon": "^5.0.0",
    "del": "^5.1.0",
    "karma": "^4.4.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-cli": "^2.0.0",
    "karma-coverage": "^2.0.1",
    "karma-mocha": "^1.3.0",
    "karma-rollup-preprocessor": "^7.0.2",
    "mocha": "^6.2.2",
    "rollup": "^1.27.13",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "snazzy": "^8.0.0",
    "standard": "^14.3.1"
  },
  "standard": {
    "env": {
      "browser": true
    },
    "ignore": [
      "examples/**",
      "docs/**",
      "build/**",
      "lib/locations/location-bar.js",
      "tests/location-bar/**"
    ]
  }
}
