{
  "private": true,
  "name": "cherrytreex",
  "version": "3.2.0",
  "description": "Cherrytree - a flexible hierarchical client side router",
  "main": "cherrytree.js",
  "module": "cherrytree.js",
  "scripts": {
    "build": "standard | snazzy && node tasks/build.js",
    "lint": "standard",
    "test-coverage": "karma start --single-run",
    "test": "karma start --single-run --no-coverage",
    "test:debug": "karma start --browsers ChromeDebugging --no-single-run --no-coverage"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/QubitProducts/cherrytree.git"
  },
  "author": "Karolis Narkevicius <karolis.n@gmail.com>",
  "contributors": [
    "Luiz Américo Pereira Câmara"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/QubitProducts/cherrytree/issues"
  },
  "dependencies": {
    "path-to-regexp": "^2.1.0"
  },
  "keywords": [
    "router",
    "history",
    "browser",
    "pushState",
    "hierarchical",
    "nested"
  ],
  "devDependencies": {
    "@sinonjs/referee": "^2.6.0",
    "@sinonjs/referee-sinon": "^4.1.0",
    "babel": "^6.5.2",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-eslint": "^8.0.3",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.18.0",
    "babel-runtime": "^6.20.0",
    "istanbul-instrumenter-loader": "^3.0.1",
    "jquery": "^3.2.1",
    "karma": "^3.1.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "^1.0.1",
    "karma-coverage": "^1.1.2",
    "karma-effroi": "0.0.0",
    "karma-firefox-launcher": "~1.1.0",
    "karma-mocha": "^1.3.0",
    "karma-rollup-preprocessor": "^6.1.0",
    "karma-sourcemap-loader": "^0.3.4",
    "karma-webpack": "^3.0.5",
    "mocha": "^5.2.0",
    "rollup": "^0.66.6",
    "rollup-plugin-babel": "3",
    "snazzy": "^8.0.0",
    "standard": "^12.0.1",
    "webpack": "^4.23.1",
    "yargs": "^10.0.3"
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "examples/**",
      "docs/**",
      "build/**",
      "lib/locations/location-bar.js",
      "tests/location-bar/**"
    ]
  }
}
