{
  "private": true,
  "name": "cherrytreex",
  "version": "3.5.0",
  "description": "Cherrytree - a flexible hierarchical client side router",
  "main": "cherrytree.js",
  "module": "cherrytree.js",
  "scripts": {
    "build": "standard | snazzy && node tasks/build.js",
    "lint": "standard",
    "test-coverage": "karma start --browsers ChromeHeadless --single-run",
    "test": "karma start --browsers ChromeHeadless --single-run --no-coverage",
    "test:debug": "karma start --browsers ChromeDebugging --no-single-run --no-coverage"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/QubitProducts/cherrytree.git"
  },
  "author": "Karolis Narkevicius <karolis.n@gmail.com>",
  "contributors": [
    "Luiz Américo Pereira Câmara"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/QubitProducts/cherrytree/issues"
  },
  "dependencies": {
    "path-to-regexp": "^2.1.0"
  },
  "keywords": [
    "router",
    "history",
    "browser",
    "pushState",
    "hierarchical",
    "nested"
  ],
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@sinonjs/referee": "^3.2.0",
    "@sinonjs/referee-sinon": "^5.0.0",
    "babel-eslint": "^10.0.1",
    "karma": "^4.0.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "^2.0.0",
    "karma-coverage": "^1.1.2",
    "karma-mocha": "^1.3.0",
    "karma-rollup-preprocessor": "^7.0.0",
    "mocha": "^6.0.2",
    "rollup": "^1.6.0",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.2.1",
    "rollup-plugin-node-resolve": "^4.0.1",
    "snazzy": "^8.0.0",
    "standard": "^12.0.1",
    "yargs": "^13.2.2"
  },
  "standard": {
    "parser": "babel-eslint",
    "env": {
      "browser": true
    },
    "ignore": [
      "examples/**",
      "docs/**",
      "build/**",
      "lib/locations/location-bar.js",
      "tests/location-bar/**"
    ]
  }
}
